#%RAML 1.0 DataType

type: object
properties
  employeeID:
    required: true
    type: integer
    example: 35355
  employeeName:
    required: true
    type: string
    example: "regerg"
  employeeStatus:
    required: true
    type: string
    example: "A"
  empLocation:
    required: true
    type: string
    example: "delhi"
example:
      {
        "employeeID": 100,
        "employeeName": "ABC",
        "employeeStatus": "A",
        "empLocation": "Mumbai"
      } 